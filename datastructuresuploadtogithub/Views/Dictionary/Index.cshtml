
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Dictionary</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css"> -->

</head>
<body>
    <div class="w3-sidebar w3-light-grey w3-bar-block">
        <h3 class="w3-bar-item">Dictionary</h3>
        <a href="@Url.Action("Add", "Dictionary")" class="w3-bar-item w3-button">Add one time to Dictionary</a><br />
        <a href="@Url.Action("HugeList", "Dictionary")" class="w3-bar-item w3-button">Add Huge List of Items to Dictionary</a><br />
        <a href="@Url.Action("Display", "Dictionary")" class="w3-bar-item w3-button">Display Dictionary</a><br />
        <a href="@Url.Action("Delete", "Dictionary")" class="w3-bar-item w3-button">Delete from Dictionary</a><br />
        <a href="@Url.Action("Clear", "Dictionary")" class="w3-bar-item w3-button">Clear Dictionary</a><br />
        <a href="@Url.Action("Search", "Dictionary")" class="w3-bar-item w3-button">Search Dictionary</a><br />
        <a href="@Url.Action("GoHome", "Dictionary")" class="w3-bar-item w3-button">Return to Main Menu</a>
    </div>
    <!-- print -->
    @Html.Raw((String)ViewBag.Message)
    @foreach (var sData in ViewBag.MyDictionary)
    {
        <p>@sData</p>
    }

</body>
</html>
